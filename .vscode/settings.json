{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "cSpell.words": [
    "msapplication",
    "preconnect",
    "stylelint"
  ],
  "cSpell.ignorePaths": [
    "node_modules",
    "yarn.lock",
    ".vscode",
    ".prettierignore",
    ".prettierrc.json",
    ".stylelintrc.json",
    "package.json",
    "next.config.js",
    ".env.local",
    "styles/_normalize.scss"
  ],
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#26394d99",
    "activityBar.activeBorder": "#ab547e",
    "activityBar.background": "#26394d99",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#ab547e",
    "activityBarBadge.foreground": "#e7e7e7",
    "titleBar.activeBackground": "#15202b99",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#15202b99",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#15202b99"
}
